# Ganti dengan kredensial Anda dari my.telegram.org
API_ID=
API_HASH=

# String sesi Telethon Anda. Jalankan skrip `generate_session.py` untuk membuatnya.
# (Saya akan menyediakan skrip ini jika diperlukan, atau Anda bisa membuatnya sendiri)
SESSION_STRING=

# Masukkan ID atau username dari channel tujuan
# Contoh: -1001234567890 atau 'namachannel'
DESTINATION_CHANNEL=

# Masukkan ID chat atau username tujuan untuk notifikasi error
# Bot akan mengirim pesan ke ID ini saat dimulai untuk meminta instruksi.
# Contoh: 123456789 (untuk user) atau 'username'
NOTIFICATION_CHAT_ID=


# Variabel berikut sekarang diatur secara interaktif saat bot dimulai.
# Anda tidak perlu mengisinya di sini lagi.
# SOURCE_CHANNEL=
# HISTORY_START_ID=
